{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "member-access": [true, "no-public"],
    "jsx-no-multiline-js": false,
    "arrow-parens": false,
    "interface-name": [true, "never-prefix"],
    "trailing-comma": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
